<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="City">
        <id name="id">
            <generator class="assigned"></generator>
        </id>

        <property name="country"></property>
        <property name="region"></property>
        <property name="ciity"></property>
        <property name="postal_code"></property>
        <property name="latitude"></property>
        <property name="longitude"></property>
        <property name="metro_code"></property>
        <property name="area_code"></property>

    </class>

</hibernate-mapping>