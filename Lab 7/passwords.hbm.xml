<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="passwords">
        <id name="id">
            <generator class="auto"></generator>
        </id>

        <property name="scheme"></property>
        <property name="time_per_unit"></property>
        <property name="state"></property>
        <property name="time_taken_c1"></property>
        <property name="state_c1"></property>
        <property name="time_taken_c2"></property>
        <property name="state_c2"></property>
        <property name="time_taken_c3"></property>
        <property name="state_c3"></property>
        <property name="time_taken_c4"></property>
        <property name="state_c4"></property>
        <property name="time_taken_c5"></property>
        <property name="state_c5"></property>
        <property name="time_taken_c6"></property>
        <property name="state_c6"></property>
        <property name="time_taken_c7"></property>
        <property name="state_c7"></property>
    </class>

</hibernate-mapping>